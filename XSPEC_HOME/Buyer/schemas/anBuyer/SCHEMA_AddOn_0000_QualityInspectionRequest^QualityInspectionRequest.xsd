    <xsd:complexType name="AcademicTitleCode">
        <xsd:simpleContent>
            <xsd:extension base="AcademicTitleCode.Content">
                <xsd:attribute name="listID">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:token">
                            <xsd:minLength value="1"/>
                            <xsd:maxLength value="60"/>
                        </xsd:restriction>
