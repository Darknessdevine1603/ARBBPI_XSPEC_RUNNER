                            <xsd:maxLength value="60"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="QualityInspectionRequestMsg">
        <xsd:annotation>
            <xsd:documentation xml:lang="EN">Quality Inspection Request</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="MessageHeader" type="BusinessDocumentMessageHeader" minOccurs="0"/>
            <xsd:element name="QualityInspectionRequestHeader" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="AribaExtension" type="AribaExtension" minOccurs="0"
                            maxOccurs="unbounded"/>
                        <xsd:element name="InspectionLotId"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="InspectionLotDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="CreatedBy" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="ActionCode" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="PeriodStartDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="PeriodEndDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="MaterialNumber" type="MaterialLong" minOccurs="0"/>
                        <xsd:element name="MaterialDescription" type="Description" minOccurs="0"/>
                        <xsd:element name="Plant" type="NOSC_PlantID" minOccurs="0"/>
                        <xsd:element name="PlantName" type="Description" minOccurs="0"/>
                        <xsd:element name="InspectionLotSize" type="Quantity" minOccurs="0"/>
                        <xsd:element name="InspectionType" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="InspectionTypeDescription" type="Description"
                            minOccurs="0"/>
                        <xsd:element name="PurchaseOrderNumber"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="PurchaseOrderLineNo"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="PurchaseOrderDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="VendorMaterialNumber" type="MaterialLong" minOccurs="0"/>
                        <xsd:element name="QualityManagedProcurement" type="NOSC_DocumentTypeCode"
                            minOccurs="0"/>
                        <xsd:element name="QMControlKeyCode"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="QMControlKey" type="Description" minOccurs="0"/>
                        <xsd:element name="QMCertificateTypeCode"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="QMCertificateType" type="Description" minOccurs="0"/>
                        <xsd:element name="QMCertificateFlag" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="VendorContactInfo" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="InternalID"
                                        type="NOSC_BusinessTransactionDocumentID"/>
                                    <xsd:element name="Address" type="Address"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="CompanyContactInfo" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="InternalId"
                                        type="NOSC_BusinessTransactionDocumentID"/>
                                    <xsd:element name="Address" type="Address"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="BatchId" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="VendorBatchId" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="BatchExpirationDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="BatchProductionDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="BatchInspectionDate" type="DateTime2" minOccurs="0"/>
                        <xsd:element name="BatchOriginCountry"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="BatchCharacteristics" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CharName"
                                        type="NOSC_BusinessTransactionDocumentID"/>
                                    <xsd:element name="CharValueDescription" type="Description"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ShortText" type="Text" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="QualityInspectionRequestDetails" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="AribaExtension" type="AribaExtension" minOccurs="0"
                            maxOccurs="unbounded"/>
                        <xsd:element name="OperationNumber"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="WorkCenter" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="SpecificationStatus"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="ObligatoryConfirmation" type="NOSC_DocumentTypeCode"
                            minOccurs="0"/>
                        <xsd:element name="CharacteristicType" type="NOSC_DocumentTypeCode"
                            minOccurs="0"/>
                        <xsd:element name="InspectionPoint"
                            type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                        <xsd:element name="InspectionCharacteristic" type="NOSC_DocumentTypeCode"
                            minOccurs="0"/>
                        <xsd:element name="InspectionCharDescription" type="Description"
                            minOccurs="0"/>
                        <xsd:element name="TargetValues" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="DecimalPlaces" type="NOSC_BusinessTransactionDocumentID"
                            minOccurs="0"/>
                        <xsd:element name="UpperTotalLimit" type="Quantity" minOccurs="0"/>
                        <xsd:element name="LowerTotalLimit" type="Quantity" minOccurs="0"/>
                        <xsd:element name="CodeList" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CodeGroup"
                                        type="NOSC_BusinessTransactionDocumentID" minOccurs="0"/>
                                    <xsd:element name="CodeText" type="Text" minOccurs="0"/>
                                    <xsd:element name="Code" type="NOSC_DocumentTypeCode"
                                        minOccurs="0"/>
                                    <xsd:element name="CodeValuation" type="NOSC_DocumentTypeCode"
                                        minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SingleResult" type="NOSC_DocumentTypeCode" minOccurs="0"/>
                        <xsd:element name="SampleSize" type="MaterialInspectionSampleID"
                            minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Attachments" type="Attachments" minOccurs="0"/>
            <xsd:element name="AribaComment" type="AribaComment" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
