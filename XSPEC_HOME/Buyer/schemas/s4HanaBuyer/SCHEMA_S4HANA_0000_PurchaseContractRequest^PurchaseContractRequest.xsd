            <xsd:element name="ProfitabilitySegment" minOccurs="0" type="ProfitabilitySegment_GFN"/>
            <xsd:element name="ProfitCenter" minOccurs="0" type="ProfitCenter_GFN"/>
            <xsd:element name="ProjectNetwork" minOccurs="0" type="ProjectNetwork_GFN"/>
            <xsd:element name="ProjectNetworkInternalID" minOccurs="0"
                type="ProjectNetworkInternalID_GFN"/>
