<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://sap.com/xi/Procurement"
    xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:3.0"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://sap.com/xi/Procurement">
    <xsd:element name="PurchaseContractRequest" type="PurchaseContractMessage"/>
    <xsd:complexType name="AccLineQuantity">
        <xsd:annotation>
            <xsd:documentation xml:lang="EN">
                <ccts:RepresentationTerm>Quantity </ccts:RepresentationTerm>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AccLineQuantity.Content">
                <xsd:attribute name="unitCode" type="MeasureUnitCode"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:simpleType name="AccLineQuantity.Content">
        <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="13"/>
            <xsd:fractionDigits value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="AccountAssigmentEntity">
        <xsd:sequence>
            <xsd:element name="ActionCode" minOccurs="0" type="ActionCode"/>
            <xsd:element name="MultipleAcctAssgmtDistrQty" minOccurs="0" type="AccLineQuantity"/>
            <xsd:element name="MultipleAcctAssgmtDistrPercent" minOccurs="0"
                type="AccountingDistrPercent"/>
            <xsd:element name="NetValueAmount" minOccurs="0" type="Amount"/>
            <xsd:element name="BusinessArea" minOccurs="0" type="BusinessArea_GFN"/>
            <xsd:element name="CommitmentItem" minOccurs="0" type="CommitmentItem_GFN"/>
            <xsd:element name="ControllingArea" minOccurs="0" type="ControllingArea_GFN"/>
            <xsd:element name="CostCenter" minOccurs="0" type="CostCenter_GFN"/>
            <xsd:element name="CostObject" minOccurs="0" type="CostObject_GFN"/>
            <xsd:element name="EarmarkedFundsDocument" minOccurs="0"
                type="EarmarkedFundsDocument_GFN"/>
            <xsd:element name="FixedAsset" minOccurs="0" type="FixedAssetID"/>
            <xsd:element name="Fund" minOccurs="0" type="Fund_GFN"/>
            <xsd:element name="FundsCenter" minOccurs="0" type="FundsCenter_GFN"/>
            <xsd:element name="GLAccount" minOccurs="0" type="GLAccount_GFN"/>
            <xsd:element name="MasterFixedAsset" minOccurs="0" type="MasterFixedAssetID"/>
            <xsd:element name="NetworkActivity" minOccurs="0" type="NetworkActivity_GFN"/>
            <xsd:element name="NetworkActivityInternalID" minOccurs="0"
                type="NetworkActivityInternalID_GFN"/>
