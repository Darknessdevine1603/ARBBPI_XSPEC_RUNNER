        <xsd:annotation>
            <xsd:documentation xml:lang="EN">IDT-Purchase Order ERP Request Other
                Limit</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Amount" type="Amount" minOccurs="0"/>
            <xsd:element name="AmountUnlimitedIndicator" type="Indicator" minOccurs="0"/>
        </xsd:sequence>
