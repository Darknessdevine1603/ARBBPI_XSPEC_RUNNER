        <xsd:sequence>
            <xsd:element name="Amount" type="Amount"/>
            <xsd:element name="BaseQuantity" type="Quantity" minOccurs="0"/>
            <xsd:element name="BaseQuantityTypeCode" type="QuantityTypeCode" minOccurs="0"/>
