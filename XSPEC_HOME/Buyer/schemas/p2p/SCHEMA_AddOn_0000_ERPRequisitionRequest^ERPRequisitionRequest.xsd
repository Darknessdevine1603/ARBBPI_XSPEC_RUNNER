            <xsd:documentation xml:lang="EN">Purchase Order ERP Accounting Coding Block
                Distribution</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AccountingCodingBlockAssignment" type="AcctgCodgBlkAssgmt"
                minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
